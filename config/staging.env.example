# Blupi Staging Environment Variables
# Copy this file and update with your staging-specific values

# Environment Configuration
NODE_ENV=staging
ENVIRONMENT=staging

# Database Configuration (Staging)
DATABASE_URL=postgresql://username:password@host:port/staging_database?sslmode=require

# Firebase Configuration (Staging Project)
VITE_FIREBASE_API_KEY=your_staging_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-staging-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-staging-project
VITE_FIREBASE_APP_ID=your_staging_app_id

# Firebase Admin SDK (Staging Service Account)
FIREBASE_PROJECT_ID=your-staging-project
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@your-staging-project.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_staging_private_key\n-----END PRIVATE KEY-----\n"

# API Keys (Can reuse from production or create staging-specific)
VITE_GOOGLE_AI_API_KEY=your_google_ai_api_key

# Optional: Staging-specific configurations
STAGING_BANNER_ENABLED=true
DEBUG_MODE=true
RATE_LIMIT_ENABLED=false

# Session Configuration
SESSION_SECRET=your_staging_session_secret_different_from_production